{
  "name": "coding-agent-fabric",
  "version": "0.1.0",
  "private": true,
  "description": "Universal resource manager for AI coding agents.",
  "keywords": [
    "ai",
    "coding-agents",
    "skills",
    "plugins"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "pnpm --recursive build",
    "clean": "rimraf \"packages/**/dist\"",
    "format": "pnpm format:trunk",
    "format:all": "pnpm format:trunk-all",
    "format:eslint": "eslint . --fix",
    "format:prettier": "prettier --write .",
    "format:trunk": "trunk fmt",
    "format:trunk-all": "trunk fmt --all",
    "lint": "pnpm lint:trunk && pnpm lint:eslint",
    "lint:all": "pnpm lint:trunk-all && pnpm lint:eslint",
    "lint:eslint": "eslint .",
    "lint:security": "trunk check --all --scope security",
    "lint:trunk": "trunk check -y",
    "lint:trunk-all": "trunk check --all -y",
    "test": "vitest run",
    "test:integration": "make -C integration_tests test",
    "test:integration:verbose": "make -C integration_tests test VERBOSE=true"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.3.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=24.13.0",
    "pnpm": ">=10.28.1"
  }
}
